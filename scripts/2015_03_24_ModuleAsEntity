#ajustes em vision del halcon
insert into Module (school, sigla, flagFormation, descriptionPT, descriptionES, descriptionEN, descriptionIT, descriptionFR) values ('VH', null, 0, 'Vision de Halcon (4 dias)', 'Vision de Halcon (4 dias)', 'Vision de Halcon (4 dias)', 'Vision de Halcon (4 dias)', 'Vision de Halcon (4 dias)');

update event set type='COMPLEMENTAR', name='Visi√≥n del Halcon', module_id=43, module=null where school = 'VH' and module_id is null;

#ajustes em coaching
update event set module_id=10, module='M3_M4' where type='FORMATION' and school='CE' and module in ('M3', 'M4') and module_id is null;
update event set module_id=9, module='M1_M2' where type='FORMATION' and school='CE' and module in ('M1','M2') and module_id is null;




#gera os update para production:
select concat('update production set module_id=', m.id, ' where school=''', m.school, ''' and module=''', m.sigla, ''';') from Module m;


#gera os update para CertificateConfigItem:
delete from CertificateConfigItem where school='EM';

#ajusta Certificate Config Item
alter table CertificateConfigItem modify column module varchar(20) null;
update CertificateConfigItem set module='M1_M2' where school='CE' and module='M1';
update CertificateConfigItem set module='M1_M2' where school='CE' and module='M2';
update CertificateConfigItem set module='M3_M4' where school='CE' and module='M3';
update CertificateConfigItem set module='M3_M4' where school='CE' and module='M4';

#gera os update para CertificateNumber
delete from CertificateNumber where school='EM';

select concat('update CertificateNumber set module_id=', m.id, ' where school=''', m.school, ''' and module=''', m.sigla, ''';') from Module m;


#genera modulos para eventos en inscripciones
select concat('update event set module_id=', m.id, ' where school=''', m.school, ''' and module=''', m.sigla, ''';') from Module m;



